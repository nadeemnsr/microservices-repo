package com.design.factory.serrviceImpl;

import com.design.factory.Computer;

public class Server implements Computer {

	String hdd;
	String ram;
	String color;
	boolean isEnabled;

	private Server() {
	}

	public Server(String hdd, String ram, String color, boolean isEnabled) {

		this.hdd = hdd;
		this.ram = ram;
		this.color = color;
		this.isEnabled = isEnabled;
	}

	@Override
	public String hdd() {
		// TODO Auto-generated method stub
		return hdd;
	}

	@Override
	public String ram() {
		// TODO Auto-generated method stub
		return ram;
	}

	@Override
	public String color() {
		// TODO Auto-generated method stub
		return color;
	}

	@Override
	public boolean isEnabled() {
		// TODO Auto-generated method stub
		return false;
	}
}
